@model UpravljanjeCekanjem.Models.Tiket
@{
    ViewBag.Title = "IzdajTiket";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.showmenu = false;
}
<script>
    $(function () {
        var serv = $.connection.cvorHub;
        $.connection.hub.start().done(function () {
            console.log('nene ');
            serv.server.osvjezi_screen_tip();
            serv.server.osvjezi_izdane("@Model.tip")
            
        });

    });
</script>


<div align="center">
    <h2>IzdajTiket</h2>
    <hr/>
    <div style="border:dashed;border-color:#4F51DB;width:400px;padding:20px" align="left">
        <h3>Redni broj tiketa: @Model.redniBroj</h3><hr/>
        <p>Vrijeme izdavanja: @Model.vrijemeIzdavanja</p>
        <p>Tip: @Model.tip</p>
        <p>Trenutni broj stranaka ispred Vas: @ViewBag.brojuredu</p>
        <p>Očekivano vrijeme dolaska na red: @ViewBag.ocekivano</p>
    </div>
    <form action="/HomeClient">
        <button class="large primary" type="submit" id="natrag" style="margin-top:20px">Natrag</button>
    </form>
</div>